olor'\" checked>\n\t\t<input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#0e0e0e\" condition=\"overlay && bg.type !== 'color'\">\n\t\t<input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n\t\t<!-- End block parameters -->\n\t</mbr-parameters>\n\n\t<div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n\t</div>\n\n\t<div class=\"container\">\n\t\t<div class=\"row mbr-white justify-content-center\">\n\t\t\t<div class=\"col-md-6 col-lg-4\" mbr-if=\"menuItems1 > 0\">\n\t\t\t\t<div class=\"item-content\">\n\t\t\t\t\t<div class=\"icon-wrap icon-padding\" mbr-if=\"menuItems2 > 0\">\n\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont mobi-mbri-map-pin mobi-mbri\"></span>\n\t\t\t\t\t\t<h6 mbr-text class=\"location mbr-regular mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".location, .icon-wrap\">570 8th Ave&nbsp;<br>New York, NY 10018, US</h6>\n\t\t\t\t\t</div>\n\n\t\t\t\t\t<div class=\"icon-wrap icon-padding\" mbr-if=\"menuItems2 > 1\">\n\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont mobi-mbri-phone mobi-mbri\"></span>\n\t\t\t\t\t\t<h6 mbr-text class=\"location mbr-regular mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".location, .icon-wrap\"><a href=\"#\" class=\"text-success\"><b>(850)\n\t\t\t\t\t\t\t\t\t435-4155</b></a></h6>\n\t\t\t\t\t</div>\n\n\t\t\t\t\t<div class=\"icon-wrap icon-padding\" mbr-if=\"menuItems2 > 2\">\n\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont mobi-mbri-clock mobi-mbri\"></span>\n\t\t\t\t\t\t<h6 mbr-text class=\"location mbr-regular mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".location, .icon-wrap\">Monday – Friday\n\t\t\t\t\t\t\t<div>10.00 AM – 11.00 PM</div>\n\t\t\t\t\t\t</h6>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\n\t\t\t<div class=\"col-md-6 col-l